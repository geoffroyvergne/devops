[lxc-workers]
lxc-k8s-worker-1 ansible_ssh_host=192.168.33.15 ansible_ssh_port=2010
lxc-k8s-worker-2 ansible_ssh_host=192.168.33.15 ansible_ssh_port=2011

[lxc-containers:children]
lxc-masters
lxc-workers

[vagrantboxes:vars]
