[vagrantboxes]
lxc-ubuntu ansible_ssh_host=192.168.33.15 ansible_ssh_private_key_file=~/dev/devops/vagrant/lxc-ubuntu/.vagrant/machines/default/virtualbox/private_key

[lxc-masters]
